<h1>Show User</h1>

<ul>

  <li>
    <strong>Nome:</strong>
    <%= @user.nome %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Decricao:</strong>
    <%= @user.decricao %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.user_path(@conn, :index) %></span>
